{
  "name": "local-first-effect-starter",
  "description": "A local-first PWA boilerplate using Effect-TS, Lit, and Replicache.",
  "type": "module",
  "author": "effect-flow-dev <effect-flow-dev@proton.me>",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"bun run dev:client\" \"bun run dev:server\"",
    "dev:client": "vite",
    "dev:server": "./scripts/wait-for-postgres.sh && bun run --watch src/server/index.ts",
    "build": "vite build",
    "start": "bun run src/server/index.ts",
    "lint": "eslint . --ext .ts,.js --cache --fix",
    "check-types": "tsc --noEmit",
    "db:migrate": "bun run src/db/migrator.ts up",
    "db:migrate:down": "bun run src/db/migrator.ts down",
    "db:prepare-types": "bun run src/db/migrator.ts up tenant_template",
    "db:generate": "bun run db:generate:central && bun run db:generate:tenant",
    "db:generate:central": "bunx kanel --config .kanelrc.central.cjs",
    "db:generate:tenant": "bun run db:prepare-types && bunx kanel --config .kanelrc.tenant.cjs --database \"$(grep DATABASE_URL_LOCAL .env | cut -d '=' -f2)?search_path=tenant_template\"",
    "db:seed": "bun run src/db/seed.ts",
    "migrate": "bun run db:migrate && bun run db:generate && bun run db:seed",
    "migrate:live": "bun run db:migrate && bun run db:seed",
    "test": "vitest run",
    "test:client": "vitest run src/components src/lib/client",
    "test:node": "vitest run src/features src/lib/server src/db",
    "test:browser": "web-test-runner --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "bun run test && bun run test:browser && bun run test:e2e",
    "open:android": "export $(grep -v '^#' .env | xargs) && cap open android"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.2",
    "@effect/vitest": "^0.24.1",
    "@open-wc/testing": "^4.0.0",
    "@playwright/test": "^1.42.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.2.18",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.8.6",
    "@types/pg": "^8.15.4",
    "@types/sinon": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@web/dev-server-esbuild": "^1.0.4",
    "@web/test-runner": "^0.20.2",
    "autoprefixer": "^10.4.21",
    "bun-types": "^1.2.18",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-lit-a11y": "^5.1.0",
    "eslint-plugin-wc": "^3.0.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sinon": "^21.0.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.4",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^3.2.4",
    "workbox-cacheable-response": "^7.4.0",
    "workbox-expiration": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0",
    "workbox-window": "^7.4.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.844.0",
    "@aws-sdk/lib-storage": "^3.844.0",
    "@capacitor/android": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/ios": "^7.4.2",
    "@capacitor/status-bar": "^7.0.2",
    "@capawesome/capacitor-android-edge-to-edge-support": "^7.2.2",
    "@effect/opentelemetry": "^0.54.0",
    "@effect/platform": "^0.87.12",
    "@effect/platform-browser": "^0.67.12",
    "@effect/platform-bun": "^0.72.17",
    "@effect/rpc": "^0.64.13",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/eden": "^1.4.5",
    "@elysiajs/static": "^1.4.7",
    "@floating-ui/dom": "^1.7.4",
    "@opentelemetry/sdk-logs": "^0.203.0",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@opentelemetry/sdk-trace-web": "^2.0.1",
    "@preact/signals-core": "^1.11.0",
    "@tiptap/core": "^3.0.7",
    "@tiptap/html": "^3.0.7",
    "@tiptap/pm": "^3.0.7",
    "@tiptap/starter-kit": "^3.0.7",
    "@types/turndown": "^5.0.5",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@zag-js/core": "^1.31.0",
    "@zag-js/select": "^1.31.0",
    "@zag-js/store": "^1.31.0",
    "@zag-js/types": "^1.31.0",
    "@zag-js/utils": "^1.31.0",
    "dotenv": "^17.2.0",
    "effect": "^3.16.12",
    "elysia": "^1.4.18",
    "idb-keyval": "^6.2.2",
    "jwt-decode": "^4.0.0",
    "kanel": "^3.14.1",
    "kanel-kysely": "^0.7.1",
    "koa-proxies": "^0.12.4",
    "kysely": "^0.28.2",
    "lit": "^3.3.1",
    "marked": "^16.1.1",
    "oslo": "^1.2.1",
    "pg": "^8.16.3",
    "replicache": "^15.3.0",
    "turndown": "^7.2.0"
  },
  "imports": {
    "#app/actions": {
      "storybook": "./actions/login-action.mock.ts",
      "default": "./actions/login-action.ts"
    },
    "#*": [
      "./*",
      "./*.ts",
      "./*.tsx"
    ]
  }
}
